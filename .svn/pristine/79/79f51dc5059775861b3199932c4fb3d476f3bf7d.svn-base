mport uniRate from \"../../components/uni-rate/uni-rate.vue\"\r\n\texport default {\r\n\t\tname: \"WorKerAllDetail\",\r\n\t\tcomponents: {\r\n\t\t\tuniRate\r\n\t\t},\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tdemo: {\r\n\t\t\t\t\ttop: 0,\r\n\t\t\t\t\theight: 0\r\n\t\t\t\t},\r\n\t\t\t\tstars: 3,\r\n\t\t\t\tindex: 0,\r\n\t\t\t\twidth: '',\r\n\t\t\t\tremark: false,\r\n\t\t\t\tprogress: {},\r\n\t\t\t\tnurse_id: \"\",\r\n\t\t\t\tperscent: 50,\r\n\t\t\t\tisChange: false,\r\n\t\t\t\tevaluate: false, // 是否点击评价按钮\r\n\t\t\t\tevaluationed: false, // 是否已评价\r\n\t\t\t\tparams: [],\r\n\t\t\t\tnurse: {},\r\n\t\t\t\tdate: new Date().toISOString().slice(0, 10),\r\n\t\t\t\tdetail: {}, // 列表信息\r\n\t\t\t\ttoken: '',\r\n\t\t\t\ttext_val: '',\r\n\t\t\t\tremark_val: '', // 更换原因\r\n\t\t\t\trate_star: 0,\r\n\t\t\t\thg: \"\",\r\n\t\t\t\tchorce: false, // 选择护工\r\n\t\t\t\tnurseList: [],\r\n\t\t\t\torder_status: '',\r\n\t\t\t\tunit: '', // 进度单位\r\n\t\t\t\tfinsh_confirm: false // 完工确认\r\n\t\t\t}\r\n\t\t},\r\n\t\tonLoad: async function(option) {\r\n\t\t\tthis.params.appoint_id = option.appoint_id;\r\n\t\t\tthis.params.nurse_id = option.nurse_id;\r\n\t\t\tthis.params.order_no = option.order_no;\r\n\t\t\tthis.params.caregivers_id = option.caregivers_id;\r\n\t\t\tthis.params.appointment_id = option.appointment_id;\r\n\t\t\tthis.order_status = option.order_status;\r\n\t\t\tthis.unit = option.unit;\r\n\t\t\tswitch (this.unit) {\r\n\t\t\t\tcase \"1\":\r\n\t\t\t\t\tthis.unit = \"天\";\r\n\t\t\t\t\tbreak;\r\n\t\t\t\tcase \"2\":\r\n\t\t\t\t\tthis.unit = \"月\";\r\n\t\t\t\t\tbreak;\r\n\t\t\t\tcase \"10\":\r\n\t\t\t\t\tthis.unit = \"个\";\r\n\t\t\t\t\tbreak;\r\n\t\t\t\tcase \"11\":\r\n\t\t\t\t\tthis.unit = \"千克\";\r\n\t\t\t\t\tbreak;\r\n\t\t\t\tcase \"12\":\r\n\t\t\t\t\tthis.unit = \"套\";\r\n\t\t\t\tdefault:\r\n\t\t\t\t\tthis.unit = \"小时\";\r\n\t\t\t\t\tbreak;\r\n\t\t\t}\r\n\r\n\t\t\tawait this.getNurseDetail();\r\n\t\t\tthis.getNurser();\r\n\t\t\tthis.roleAPI();\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t\tconst demo = uni.getMenuButtonBoundingClientRect()\r\n\t\t\tthis.demo.top = demo.top\r\n\t\t\tthis.demo.height = demo.height\r\n\t\t\tif (app.globalData.token == undefined) {\r\n\t\t\t\tthis.token = uni.getStorageSync('token');\r\n\t\t\t} else {\r\n\t\t\t\tthis.token = app.globalData.token;\r\n\t\t\t}\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\tbackleft(e) {\r\n\t\t\t\tuni.navigateBack();\r\n\t\t\t},\r\n\t\t\tchangeStarts(num) {\r\n\t\t\t\tthis.stars = num.value;\r\n\t\t\t},\r\n\t\t\t// 更换\r\n\t\t\tchangeFn() {\r\n\t\t\t\tif (this.nurseList.length > 0) {\r\n\t\t\t\t\tthis.remark = !this.remark;\r\n\t\t\t\t} else {\r\n\t\t\t\t\tuni.showToast({\r\n\t\t\t\t\t\ttitle: \"没有可更换的护工\",\r\n\t\t\t\t\t\ticon: \"none\"\r\n\t\t\t\t\t});\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\tradioChange(e) {\r\n\t\t\t\tthis.nurse_id = e.detail.value\r\n\t\t\t},\r\n\t\t\t// 用户角色显示【确认完工】功能\r\n\t\t\tasync roleAPI(index) {\r\n\t\t\t\tconst res = await this.$myRequset({\r\n\t\t\t\t\turl: \"/api\",\r\n\t\t\t\t\tmethod: \"POST\",\r\n\t\t\t\t\tdata: {\r\n\t\t\t\t\t\toperate: \"MemberCenter.member_bing\",\r\n\t\t\t\t\t\ttoken: this.token,\r\n\t\t\t\t\t}\r\n\t\t\t\t})\r\n\t\t\t\tvar role = res.data.data.member;\r\n\t\t\t\trole.forEach((val) => {\r\n\t\t\t\t\tif (val.role == 1) {\r\n\t\t\t\t\t\tthis.finsh_confirm = true;\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\tthis.finsh_confirm = false;\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\t\t\t},\r\n\t\t\tasync finishFn() {\r\n\t\t\t\tconst res = await this.$myRequset({\r\n\t\t\t\t\turl: \"/api\",\r\n\t\t\t\t\tmethod: \"POST\",\r\n\t\t\t\t\tdata: {\r\n\t\t\t\t\t\toperate: \"Member.AppointOrder.up_finish\",\r\n\t\t\t\t\t\ttoken: this.token,\r\n\t\t\t\t\t\tappointment_id: this.params.appointment_id,\r\n\t\t\t\t\t\tnurse_id: this.params.nurse_id,\r\n\t\t\t\t\t\torder_no: this.params.order_no\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\t\t\t\tif (res.data.code == 1) {\r\n\t\t\t\t\tuni.redirectTo({\r\n\t\t\t\t\t\turl: \"../worker/worker\"\r\n\t\t\t\t\t})\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\t// 确认完工\r\n\t\t\tverify