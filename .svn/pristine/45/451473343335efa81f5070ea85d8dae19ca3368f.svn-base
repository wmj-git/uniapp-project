.demo {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  background: #fff;
  font-size: 26rpx;
  width: 100%;
  text-align: center;
  position: fixed;
  top: 0;
  z-index: 999;
}
.demo .left {
  float: left;
  position: absolute;
  width: -webkit-max-content;
  width: max-content;
  height: -webkit-max-content;
  height: max-content;
  top: 0;
  bottom: 0;
  left: 20rpx;
  margin: auto;
}
.demo .icon-zuo {
  color: #000;
}
.pd_t123 {
  padding-top: 123rpx;
}
.container {
  padding: 30rpx;
  background: #fff;
}
.container .texts {
  color: #8e8e8e;
  font-size: 26rpx;
  padding-top: 60rpx;
}
.container .texts .text_item {
  padding-top: 20rpx;
}
.container .texts .strong {
  display: inline-block;
  color: #000;
  font-weight: bold;
}
.container .grad {
  background-image: -webkit-linear-gradient(left, #63cce7, #66ebdf);
  background-image: linear-gradient(to right, #63cce7, #66ebdf);
}
.container .agree {
  width: 374rpx;
  height: 90rpx;
  color: #fff;
  border: none;
  border-radius: 40rpx;
}
.container .noagree {
  display: block;
  text-align: center;
  background: transparent;
  color: #8e8e8e;
}
.container .pt50 {
  padding-top: 30px;
}
.bar-line {
  height: 44px;
}
.bar-line .bar-back {
  font-size: 52rpx !important;
  position: absolute;
  left: 30rpx;
}
.bar-line .bar-title {
  font-size: 32rpx;
}

