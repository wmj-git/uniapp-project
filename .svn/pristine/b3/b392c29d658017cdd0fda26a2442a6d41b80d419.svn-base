olor({\r\n\t\t\t\tfrontColor: '#000000',\r\n\t\t\t\tbackgroundColor: '#000000'\r\n\t\t\t})\r\n\t\t},\r\n\t\tprops: {\r\n\t\t\tactiveColor: {\r\n\t\t\t\t// 星星选中状态颜色\r\n\t\t\t\ttype: String,\r\n\t\t\t\tdefault: \"#ffca3e\"\r\n\t\t\t},\r\n\t\t\tsize: {\r\n\t\t\t\t// 星星的大小\r\n\t\t\t\ttype: [Number, String],\r\n\t\t\t\tdefault: 14\r\n\t\t\t}\r\n\t\t},\r\n\t\tdata(){\r\n\t\t\treturn{\r\n\t\t\t\ttoken:\"\",\r\n\t\t\t\tphone:'',\r\n\t\t\t\tnickName:\"\",\r\n\t\t\t\tavatarUrl:\"\",\r\n\t\t\t\tfrom_mid:0,\r\n\t\t\t\tisphoneView:false,\r\n\t\t\t\tcurrent: 0,\r\n\t\t\t\tfold:true,\r\n\t\t\t\tbtnspec:true,\r\n\t\t\t\tdemo: {\r\n\t\t\t\t\ttop: 0,\r\n\t\t\t\t\theight: 0\r\n\t\t\t\t},\r\n\t\t\t\tgoods_id:\"\",\r\n\t\t\t\tgoodsdtail:[],//商品详情数据\r\n\t\t\t\tshowPicker:false,//时间状态\r\n\t\t\t\tdatetime: mytime,\r\n\t\t\t\ttype:'rangetime',//时间插件类型\r\n\t\t\t\tvalue:'',//时间插件\r\n\t\t\t\tnum:1,//下单数量默认是1\r\n\t\t\t\tisminNormal:false,\r\n\t\t\t\tismaxNormal:true,\r\n\t\t\t\tgoodsInfo:{},\r\n\t\t\t\t\r\n\t\t\t\ttotalPrice:{\r\n\t\t\t\t\t\"marketPrice\":0,\r\n\t\t\t\t\t\"shopPrice\":0\r\n\t\t\t\t},\r\n\r\n\t\t\t\torderParam:{},\r\n\t\t\t\tStates:{},\r\n\t\t\t\tspecs:[],\r\n\t\t\t\tspecList:[],\r\n\t\t\t\tgoods_spec:\"\",\t\r\n\t\t\t\t\r\n\t\t\t}\r\n\t\t},\r\n\t\tmethods: {\t\r\n\t\t\t// 获取微信用户信息\r\n\t\t\tgetUserinfo: function(){\t\t\t\t\r\n\t\t\t\t var that=this;\r\n\t\t\t\t uni.getUserInfo({\r\n\t\t\t\t\tsuccess: function(res) {\r\n\t\t\t\t\t\tvar userInfo = res.userInfo\r\n\t\t\t\t\t\tthat.nickName = userInfo.nickName    //昵称\r\n\t\t\t\t\t\tthat.avatarUrl = userInfo.avatarUrl //头像\r\n\t\t\t\t\t\tvar gender = userInfo.gender //性别 0：未知、1：男、2：女\r\n\t\t\t\t\t\tvar province = userInfo.province\r\n\t\t\t\t\t\tvar city = userInfo.city\r\n\t\t\t\t\t\tvar country = userInfo.country\t\t\t\t\t\t\t\t\t\t\t\t\t\t\r\n\t\t\t\t\t\tthat.isphoneView = true\r\n\t\t\t\t\t\tthat.isCanUse=false; //记录是否第一次授权  false:表示不是第一次授权\r\n\t                    app.updateUserInfo();\r\n\t\t\t\t\t\t\r\n\t\t\t\t\t},\r\n\t\t\t\