ass="label">邮政编码</view><input class="uni-input" name="input" placeholder="请输入邮编" data-event-opts="{{[['input',[['__set_model',['$0','postcode','$event',[]],['addressData']]]]]}}" value="{{addressData.postcode}}" bindinput="__e"/></view><view class="uni-form-item uni-list-cell uni-list-cell-pd default_address"><view class="uni-list-cell-db">设为默认地址</view><switch checked="{{checked}}" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><view class="uni-btn-v"><button form-type="submit" data-event-opts="{{[['tap',[['saveSubmit',['$event']]]]]}}" bindtap="__e">提交</button></view></form></view></view></view>