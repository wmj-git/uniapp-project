uni-link {
  display: block;
  opacity: 0;
}
.demo {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  background: #fff;
  font-size: 26rpx;
  width: 100%;
  text-align: center;
  position: fixed;
  top: 0;
  z-index: 999;
  color: #000;
}
.demo .left {
  float: left;
  position: absolute;
  width: -webkit-max-content;
  width: max-content;
  height: -webkit-max-content;
  height: max-content;
  top: 0;
  bottom: 0;
  left: 20rpx;
  margin: auto;
}
.demo .left .icon-zuo {
  color: #000000;
}
.info_item {
  background-color: #fff;
  margin-bottom: 20rpx;
}
.top_title {
  font-size: 34rpx;
}
.pd-t135 {
  padding-top: 135rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  position: relative;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.pd-t135 .head_cont {
  width: 680rpx;
  height: 347rpx;
  margin: 30rpx 0;
  border-radius: 10rpx;
  overflow: hidden;
  position: relative;
}
.pd-t135 .head_cont image {
  display: block;
  max-width: 680rpx;
  width: 100%;
  height: 347rpx;
  position: absolute;
  z-index: 0;
}
.pd-t135 .head_title {
  color: #fff;
  font-size: 32rpx;
  letter-spacing: 4rpx;
  position: absolute;
  overflow: hidden;
  padding: 30rpx 40rpx 0;
  display: inline-block;
  font-weight: bolder;
}
.newsImg01 {
  width: 680rpx;
  height: 347rpx;
  margin: 30rpx;
}
.newsImg {
  width: 150rpx;
  height: 150rpx;
  -webkit-align-self: center;
          align-self: center;
  padding: 20rpx 0;
}
.info {
  padding-right: 50rpx;
  padding-top: 25rpx;
  width: 70%;
  position: relative;
}
.wauto80 {
  width: 82%;
  margin: 10rpx auto 0;
  border-radius: 10rpx;
}
.imgRignt {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.w23 {
  width: 23rpx;
  height: 14rpx;
  padding-right: 10rpx;
}
.newsdata {
  padding-right: 80rpx;
  font-size: 24rpx;
}
.tag {
  background: #00c4bc;
  border-top-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  color: #fff;
  font-size: 20rpx;
  padding: 4rpx 10rpx;
  margin-right: 20rpx;
}
.title {
  display: -webkit-box;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  line-height: 40rpx;
}
.title text {
  color: #000000;
  font-size: 30rpx;
  font-weight: bold;
}
.des {
  font-size: 20rpx;
  color: #c3c3c3;
  position: absolute;
  bottom: 20rpx;
}

